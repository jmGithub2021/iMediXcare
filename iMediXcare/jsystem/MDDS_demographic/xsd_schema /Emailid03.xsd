<?xml version = "1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<xs:element name= "Email">
<!-- Reference value for its Metadata (G00.09)-->
<xs:simpleType>
<xs:restriction base= "xs:string">
<xs:pattern value="[0-9A-Za-z'\.\-_]{1,100}@[0-9A-Za-z'\.\-_]{1,100}\.[A-Za-z]{1,54}"/>

</xs:restriction>
</xs:simpleType>
</xs:element>
</xs:schema>