<?xml version ="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

<xs:element name="Martial_Status">
<!-- Reference value for its Metadata (G01.04)-->
<xs:simpleType>

<xs:restriction base= "xs:string">
<xs:maxLength value="20"/>
<xs:enumeration value= "1-Nevermarried"/>
<xs:enumeration value= "2-Currentlymarried"/>
<xs:enumeration value= "3-widow/widower"/>
<xs:enumeration value= "4-Divorced"/>
